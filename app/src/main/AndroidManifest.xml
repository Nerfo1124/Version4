<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.usuario.version4" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".InicioSesion"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RegistroUsuario"
            android:label="@string/title_activity_registro_usuario" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".InicioSesion" />
        </activity>
        <activity
            android:name=".Conformula"
            android:label="@string/title_activity_conformula" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".RegistroUsuario" />
        </activity>
        <activity
            android:name=".Sinformulauno"
            android:label="@string/title_activity_sinformulauno" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".RegistroUsuario" />
        </activity>
        <activity
            android:name=".Sinformulados"
            android:label="@string/title_activity_sinformulados" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Sinformulauno" />
        </activity>
        <activity
            android:name=".Inicio"
            android:label="@string/title_activity_inicio" >
        </activity>
    </application>

</manifest>
